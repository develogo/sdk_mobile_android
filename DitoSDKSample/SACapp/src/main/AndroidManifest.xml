<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ioasys.com.br.sacapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".activities.MainScreen"
            android:label="@string/app_name"
            android:theme="@style/MyAppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainPage"
            android:label="@string/title_activity_main_page" >
        </activity>
        <activity
            android:name=".activities.Chat"
            android:label="@string/title_activity_chat" >
        </activity>
        <activity
            android:name=".activities.OpenedCall"
            android:label="@string/title_activity_call_open" >
        </activity>
    </application>

</manifest>
